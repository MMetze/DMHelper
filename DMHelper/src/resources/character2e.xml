<?xml version="1.0" encoding="UTF-8"?>
<dmh:template xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="calendar.xsd">
    <dmh:attribute type="dmh:string" name="_baseID"/>
    <dmh:attribute type="dmh:integer" name="type"/>
    <dmh:attribute type="dmh:string" name="name"/>
    <dmh:attribute type="dmh:boolean" name="active" default="1"/>
    <dmh:attribute type="dmh:boolean" name="expanded"/>
    <dmh:attribute type="dmh:integer" name="row"/>
    <dmh:attribute type="dmh:string" name="dndBeyondID" default="-1"/>
    <dmh:attribute type="dmh:string" name="icon"/>
    <dmh:attribute type="dmh:integer" name="initiative"/>
    <dmh:attribute type="dmh:string" name="size" default="medium"/>
    <dmh:attribute type="dmh:integer" name="armorClass" default="10"/>
    <dmh:attribute type="dmh:integer" name="hitPoints" default="1"/>
    <dmh:attribute type="dmh:integer" name="speed" default="12"/>
    <dmh:attribute type="dmh:integer" name="conditions"/>

    <dmh:attribute type="dmh:integer" name="strength" default="10"/>
    <dmh:attribute type="dmh:integer" name="strengthHitProbability"/>
    <dmh:attribute type="dmh:integer" name="strengthDamageAdjustment"/>
    <dmh:attribute type="dmh:integer" name="strengthWeightAllowance" default="40"/>
    <dmh:attribute type="dmh:integer" name="strengthMaximumPress" default="115"/>
    <dmh:attribute type="dmh:integer" name="strengthOpenDoors" default="6"/>
    <dmh:attribute type="dmh:integer" name="strengthBendBarsLiftGates" default="2"/>
    <dmh:attribute type="dmh:integer" name="dexterity" default="10"/>
    <dmh:attribute type="dmh:integer" name="dexterityReactionAdjustment"/>
    <dmh:attribute type="dmh:integer" name="dexterityMissileAttackAdjustment"/>
    <dmh:attribute type="dmh:integer" name="dexterityDefensiveAdjustment"/>
    <dmh:attribute type="dmh:integer" name="constitution" default="10"/>
    <dmh:attribute type="dmh:string" name="constitutionHitPointAdjustment" default="0"/>
    <dmh:attribute type="dmh:integer" name="constitutionSystemShock" default="70"/>
    <dmh:attribute type="dmh:integer" name="constitutionResurrectionSurvival" default="75"/>
    <dmh:attribute type="dmh:integer" name="constitutionPoisonSave" default="0"/>
    <dmh:attribute type="dmh:string" name="constitutionRegeneration" default="Nil"/>
    <dmh:attribute type="dmh:integer" name="intelligence" default="10"/>
    <dmh:attribute type="dmh:integer" name="intelligenceNumberOfLanguages" default="2"/>
    <dmh:attribute type="dmh:integer" name="intelligenceSpellLevel" default="5"/>
    <dmh:attribute type="dmh:integer" name="intelligenceChanceToLearnSpell" default="40"/>
    <dmh:attribute type="dmh:string" name="intelligenceMaxNumberOfSpells" default="7"/>
    <dmh:attribute type="dmh:string" name="intelligenceSpellImmunity" default="Nil"/>
    <dmh:attribute type="dmh:integer" name="wisdom" default="10"/>
    <dmh:attribute type="dmh:integer" name="wisdomMagicalDefenseAdjustment"/>
    <dmh:attribute type="dmh:string" name="wisdomBonusSpells" default="0"/>
    <dmh:attribute type="dmh:integer" name="wisdomChanceOfSpellFailure" default="15"/>
    <dmh:attribute type="dmh:string" name="wisdomSpellImmunity" default="Nil"/>
    <dmh:attribute type="dmh:integer" name="charisma" default="10"/>
    <dmh:attribute type="dmh:integer" name="charismaMaximumNumberOfHenchmen" default="4"/>
    <dmh:attribute type="dmh:integer" name="charismaLoyaltyBase"/>
    <dmh:attribute type="dmh:integer" name="charismaReactionAdjustment"/>

    <dmh:attribute type="dmh:integer" name="saveParalyzationPoisonDeathMagic"/>
    <dmh:attribute type="dmh:integer" name="saveRodStaffWand"/>
    <dmh:attribute type="dmh:integer" name="saveBreathWeapon"/>
    <dmh:attribute type="dmh:integer" name="saveSpell"/>

    <dmh:attribute type="dmh:string" name="race"/>
    <dmh:attribute type="dmh:string" name="subrace"/>
    <dmh:attribute type="dmh:string" name="class"/>
    <dmh:attribute type="dmh:string" name="level"/>
    <dmh:attribute type="dmh:string" name="experience"/>
    <dmh:attribute type="dmh:string" name="maximumHp" default="1"/>
    <dmh:attribute type="dmh:string" name="tempHp"/>
    <dmh:attribute type="dmh:string" name="player"/>
    <dmh:attribute type="dmh:string" name="eyes"/>
    <dmh:attribute type="dmh:string" name="age"/>
    <dmh:attribute type="dmh:string" name="sex"/>
    <dmh:attribute type="dmh:string" name="hair"/>
    <dmh:attribute type="dmh:string" name="height"/>
    <dmh:attribute type="dmh:string" name="weight"/>
    <dmh:attribute type="dmh:string" name="alignment"/>

    <dmh:attribute type="dmh:integer" name="platinum"/>
    <dmh:attribute type="dmh:integer" name="gold"/>
    <dmh:attribute type="dmh:integer" name="electrum"/>
    <dmh:attribute type="dmh:integer" name="silver"/>
    <dmh:attribute type="dmh:integer" name="copper"/>

    <dmh:element type="dmh:html" name="spell-data"/>
    <dmh:element type="dmh:html" name="equipment"/>
    <dmh:element type="dmh:html" name="notes"/>

    <dmh:element type="dmh:list" name="spellSlots">
        <dmh:attribute type="dmh:integer" name="level"/>
        <dmh:attribute type="dmh:resource" name="slots"/>
    </dmh:element>

    <dmh:element type="dmh:list" name="proficiencies">
        <dmh:attribute type="dmh:string" name="name"/>
        <dmh:attribute type="dmh:integer" name="value"/>
    </dmh:element>
</dmh:template>
